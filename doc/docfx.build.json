{	
	"build": {
		"content": [
			{
				"files": [
					"api/**.yml",
					"api/index.md"
				]
			},
			{
				"files": [
					"**/*.md",
					"**/*.yml",
					"toc.yml",
					"*.md",
					"restapi/**"
				],
				"exclude": [
					"obj/**",
					"_site/**",
					"tools/**"
				]
			}
		],
		"resource": [
			{
				"files": [
					"images/**",
					"files/**"
				],
				"exclude": [
					"obj/**",
					"_site/**",
					"tools/**"
				]
			}
		],
		"overwrite": [
			{
				"files": [
					"apidoc/**.md"
				],
				"exclude": [
					"obj/**",
					"_site/**",
					"tools/**"
				]
			}
		],
		"dest": "_site",
		"globalMetadataFiles": [],
		"fileMetadataFiles": [],
		"template": [
			"default",
			"template"
		],
		"globalMetadata": {
			"_appTitle": "SUGAR",
			"_enableSearch": true
		},
		"postProcessors": [],
		"noLangKeyword": false,
		"keepFileLink": false,
		"cleanupCacheHistory": false,
		"disableGitFeatures": false
	}
}